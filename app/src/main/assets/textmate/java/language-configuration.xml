<?xml version="1.0" encoding="UTF-8" ?>
<root>
	<comments>
		<lineComment>//</lineComment>
		<blockComment>/*</blockComment>
		<blockComment>*/</blockComment>
	</comments>
	<brackets>{</brackets>
	<brackets>}</brackets>
	<brackets>[</brackets>
	<brackets>]</brackets>
	<brackets>(</brackets>
