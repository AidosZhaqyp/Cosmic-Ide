	<autoClosingPairs>[</autoClosingPairs>
	<autoClosingPairs>]</autoClosingPairs>
	<autoClosingPairs>(</autoClosingPairs>
	<autoClosingPairs>)</autoClosingPairs>
	<autoClosingPairs>
		<open>"</open>
		<close>"</close>
